<UserControl x:Class="PipeLine.AdminDesktop.Views.ErrorTickets2.ErrorTicketsMenu"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PipeLine.AdminDesktop.Views.ErrorTickets2"
             mc:Ignorable="d" 
             d:DesignHeight="32" d:DesignWidth="800">
    <Border Background="{StaticResource backgroundPrimaryColor}">
        <StackPanel Grid.Row="1" Orientation="Horizontal">
            <Button 
        Style="{StaticResource subMenuButton}"
        HorizontalAlignment="Left"
        Width="300"
        Margin="10 0 40 0"
        Content="List Of Error Tickets"
        Command="{Binding ShowErrorTicketsListViewCommand}"
    />
           
            <Button 
    Style="{StaticResource subMenuButton}"
    HorizontalAlignment="Left"
    Width="300"
    Margin="10 0 40 0"
    Content="Add New ErrorTicket"
    Command="{Binding ShowAddNewErrorTicketsViewCommand}"
/>

        </StackPanel>
    </Border>
</UserControl>
